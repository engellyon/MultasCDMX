  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>MULTAS_CDMX</DataSourceName>
        <CommandText>select ISNULL(Genero,
	CASE WHEN GROUPING(Genero) =0 then 'Desconocido' else 'Total' end) as Genero,
	count(*) as Infractor
from Persona,Comete,Infraccion
where Persona.RFC = Comete.RFC and Comete.IdExpediente = Infraccion.IdExpediente
group by rollup (Genero);</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Genero">
          <DataField>Genero</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Infractor">
          <DataField>Infractor</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
